{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source":"/public/sw.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0"
          }
        ]
      }
    ],
    "redirects": [ {
      "source" : "/index.html",
      "destination" : "/",
      "type" : 302
    }, {
      "source" : "/contacto.html",
      "destination" : "/contacto",
      "type" : 302
    } ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
