doctype html
html(lang='es')
    head
        meta(name='encoding', charset='UTF-8')
        meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no')
        title Canessa peluquería canina Valdemoro
        meta(name='description', content='Canessa ofrece servicios de peluquería y belleza canina y felina con cortes de pelo comerciales y de exposición. También se realizan arreglos y lavados. Tenemos tarifa especial para autolavados además de un SPA canino. 91 248 04 30')
        meta(name='keywords', content='peluqueria canina Valdemoro, Pinto, Ciempozuelos, peluqueria canina, peluqueria animal, peluqueria perros, peluqueria gatos, piensos, complementos, correas, lazos, collares, antiparasitarios, autolavado')
        //- All (common) METATAGS in head tag
        include includes/metatags

    body
        //- ----------------------------------------------------------------
        //- 01. SLIDER Full Page
        //- ----------------------------------------------------------------
        img.main-logotype(src='img/logos/canessa-logo.svg', title='#{index.logo.alt}', alt='#{index.logo.alt}')
        .slider.fullscreen
          ul.slides
            li.first
              img(src='img/slider/afgano.jpg', title='Afgano: Canessa peluquería canina', alt='Afgano: Canessa peluquería canina')
              .caption.left-align
                h1 #{index.slides.slide_01.title}
                h5.intro #{index.slides.slide_01.subtitles}
                a.waves-effect.waves-light.btn.pink.lighten-1(href='gallery.html', title='Servicios ofrecidos por la peluquería canina Canessa') #{index.slides.slide_01.btn}
            li
              img(src='img/slider/golden-under-water.jpg', title='Servicio de autolavado de la peluquería canina Canessa', alt='Servicio de autolavado de la peluquería canina Canessa')
              .caption.left-align
                h1 #{index.slides.slide_02.title}
                h5.intro #{index.slides.slide_02.subtitles}
                a.waves-effect.waves-light.btn.pink.lighten-1(href='gallery.html', title='Servicios ofrecidos por la peluquería canina Canessa') #{index.slides.slide_02.btn}
            li
              img(src='img/slider/entrada.jpg', title='Entrada y recibidor de la Canessa peluquería canina', alt='Entrada y recibidor de la Canessa peluquería canina')
              .caption.left-align.mycaption
                h1 #{index.slides.slide_03.title}
                h5.intro #{index.slides.slide_03.subtitles}
                a.waves-effect.waves-light.btn.pink.lighten-1(href='gallery.html', title='Servicios ofrecidos por la peluquería canina Canessa') #{index.slides.slide_03.btn}
            li
              img(src='img/slider/peluqueria.jpg', title='Zona de corte y baño de la Canessa peluquería canina', alt='Zona de corte y baño de la Canessa peluquería canina')
              .caption.left-align
                h1 !{index.slides.slide_04.title}
                h5.intro #{index.slides.slide_04.subtitles}
                a.waves-effect.waves-light.btn.pink.lighten-1(href='gallery.html', title='Servicios ofrecidos por la peluquería canina Canessa') #{index.slides.slide_04.btn}

        //- ----------------------------------------------------------------
        //- 00. MAIN (Footer) NAV BAR
        //- ----------------------------------------------------------------
        .navbar-fixed
          nav
            .nav-wrapper
              //- <a href="#!" class="brand-logo">Logo</a>
              a#mobile-bars.button-collapse(href='#', data-activates='mobile-demo', title='mobile icon')
                i.fa.fa-bars
              //- LEFT SIDE FOOTER LINKS MENU
              ul.left.hide-on-med-and-down
                //- li
                  a.navbar-link(href='art-gallery.html', title='Servicios ofrecidos por la peluquería canina Canessa') Servicios
                li
                  a.navbar-link.copyright(href='#', title='Copyright de la peluquería canina Canessa') &copy; #{footer.copyright},
                    span.js-current-year
                li
                  a.navbar-link.schedule.modal-trigger(href='#modal_schedule', title='Horario comercial de la peluquería canina Canessa') #{common.titles.schedule}
                li
                  a.navbar-link.terms(href='legal.html', title='Condiciones de uso del website de la peluquería canina Canessa') #{common.titles.legal}
              //-
                li
                  a.navbar-link.jalofer(href='http://jalofernandez.com/', title='Website diseñado y desarrollado por jalofernandez.com', target="_blank") jalofernandez.com
              //- RIGHT SIDE FOOTER LINKS MENU
              ul.right
                li
                    a.navbar-link(href='https://www.facebook.com/Peluqueria-Canina-Canessa-113194982084561/', title='Facebook de la peluquería canina Canessa', target="_blank", rel="noopener noreferrer")
                        i.fa.fa-facebook
                li
                    a.navbar-link(href='https://www.instagram.com/peluqueriacanessa/', title='Instagram de la peluquería canina Canessa', target="_blank", rel="noopener noreferrer")
                        i.fa.fa-instagram
              //- MOBILE
              ul#mobile-demo.side-nav
                li
                  img(src='img/logos/canessa-logo.svg', title='Logotipo de la peluquería canina Canessa', alt='Logotipo de la peluquería canina Canessa')
                li.socialMobile
                    a.navbar-link(href='https://www.facebook.com/Peluqueria-Canina-Canessa-113194982084561/', title='Facebook de la peluquería canina Canessa', target="_blank", rel="noopener noreferrer")
                        i.fa.fa-facebook
                    a.navbar-link(href='https://www.instagram.com/peluqueriacanessa/', title='Instagram de la peluquería canina Canessa', target="_blank", rel="noopener noreferrer")
                        i.fa.fa-instagram
                li
                    a.navbar-link(href='tel:912480430', title='Llamar a la peluquería canina Canessa') #{common.phone}
                li
                    a.navbar-link.modal-trigger(href='#modal_schedule', title='Horario de la peluquería canina Canessa') #{common.titles.schedule}
                li
                    a.navbar-link(href='gallery.html', title='Servicios ofrecidos por la peluquería canina Canessa') #{common.titles.services}
                li
                    a.navbar-link(href='mascotactiva.html', title='Servicios de cuidado y entrenamiento canino en la peluquería canina Canessa') #{common.titles.mascota}
                li
                    a.navbar-link(href='legal.html', title='Términos y condiciones de uso del website de la peluquería canina Canessa') #{common.titles.legal}
                li.copyMobile
                    a.navbar-link(href='#', title='Copyright de la peluquería canina Canessa') &copy; #{footer.copy},
                        span.js-current-year
